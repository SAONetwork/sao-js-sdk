{
  "name": "@saonetwork/sid",
  "version": "0.0.2",
  "description": "sao did that support binds multiple wallets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix --ignore-pattern \"src/**/*.test.[jt]s\" \"src/**/*.[jt]s\"",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@cosmjs/proto-signing": "^0.29.4",
    "@ethersproject/hdnode": "^5.7.0",
    "@keplr-wallet/types": "^0.11.17",
    "@saonetwork/api-client": "^0.0.2",
    "@saonetwork/common": "^0.0.1",
    "@stablelib/x25519": "^1.0.3",
    "another-did-jwt": "7.0.0",
    "buffer": "^6.0.3",
    "caip": "^1.1.0",
    "did-resolver": "^3.2.2",
    "dids": "^3.4.0",
    "ethers": "^5.7.2",
    "fast-json-stable-stringify": "^2.1.0",
    "key-did-provider-ed25519": "^2.0.0",
    "key-did-resolver": "^2.0.0",
    "lru-cache": "^7.14.1",
    "uint8arrays": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SaoNetwork/saonetwork.git",
    "directory": "packages/sid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"hello\"; return 1"
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.tsx?$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  },
  "gitHead": "fb06c7e9a0e1eb5bf4cb3fb017cf070b5dcca01e"
}
