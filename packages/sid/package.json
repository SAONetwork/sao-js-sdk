{
  "name": "@js-sao-did/sid",
  "version": "0.0.0",
  "description": "sao did that support binds multiple wallets",
  "author": "LeoR <leo@sao.network>",
  "homepage": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@cosmjs/proto-signing": "^0.29.4",
    "@ethersproject/hdnode": "^5.7.0",
    "@js-sao-did/common": "^0.0.0",
    "@keplr-wallet/types": "^0.11.17",
    "@stablelib/x25519": "^1.0.3",
    "SaoNetwork-sao-client-ts": "https://github.com/rleor/sao-ts-client.git",
    "axios": "0.27.2",
    "buffer": "^6.0.3",
    "caip": "^1.1.0",
    "did-resolver": "^3.2.2",
    "dids": "^3.4.0",
    "did-jwt": "^6.10.1",
    "ethers": "^5.7.2",
    "fast-json-stable-stringify": "^2.1.0",
    "key-did-provider-ed25519": "^2.0.0",
    "key-did-resolver": "^2.0.0",
    "uint8arrays": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SaoNetwork/js-sao-did.git",
    "directory": "packages/sid"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.tsx?$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  }
}
