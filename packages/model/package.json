{
  "name": "@sao-js-sdk/model",
  "version": "0.0.0",
  "description": "sao model management",
  "author": "Not@All <well@sao.network>",
  "homepage": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@js-sao-did/api-client": "0.0.3",
    "@js-sao-did/common": "0.0.0",
    "cids": "^1.1.9",
    "fast-json-patch": "3.1.1",
    "multihashing-async": "^2.1.4"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  }
}
