{
  "name": "@saonetwork/toolkits",
  "version": "0.0.1",
  "keywords": [
    "did"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix --ignore-pattern \"src/**/*.test.[jt]s\" \"src/**/*.[jt]s\"",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^2.0.2",
    "@cosmjs/proto-signing": "^0.29.4",
    "@saonetwork/api-client": "0.0.6",
    "@saonetwork/model": "0.0.29",
    "@saonetwork/sid": "0.0.23",
    "@types/varint": "^6.0.1",
    "axios": "0.27.2",
    "caip": "^1.1.0",
    "cids": "^1.1.9",
    "dev3-sdk": "^0.0.31",
    "jest": "^29.3.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.tsx?$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  },
  "gitHead": "fb06c7e9a0e1eb5bf4cb3fb017cf070b5dcca01e"
}
